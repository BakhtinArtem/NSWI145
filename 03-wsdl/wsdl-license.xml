<!--LICENSE WSDL-->
<definitions
        xmlns:wsam="http://www.w3.org/2007/05/addressing/metadata"
        xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
        xmlns:tns="http://license.example.org/"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        xmlns="http://schemas.xmlsoap.org/wsdl/"
        targetNamespace="http://license.example.org/"
        name="LicenseGeneratorImplService"
>

    <types>
        <xsd:schema>
            <xsd:import namespace="http://license.example.org/" schemaLocation="http://127.0.0.1:8000/LICENSE?xsd=1"/>
        </xsd:schema>
    </types>

    <message name="getLicenseBySerialNumber">
        <part name="parameters" element="tns:getLicenseBySerialNumber"/>
    </message>
    <message name="getLicenseBySerialNumberResponse">
        <part name="parameters" element="tns:getLicenseBySerialNumberResponse"/>
    </message>
    <message name="generateLicense">
        <part name="parameters" element="tns:generateLicense"/>
    </message>
    <message name="generateLicenseResponse">
        <part name="parameters" element="tns:generateLicenseResponse"/>
    </message>

    <portType name="ILicenseGenerator">
        <operation name="getLicenseBySerialNumber">
            <input wsam:Action="tns:ILicenseGenerator/getLicenseBySerialNumberRequest" message="tns:getLicenseBySerialNumber"/>
            <output wsam:Action="tns:ILicenseGenerator/getLicenseBySerialNumberResponse" message="tns:getLicenseBySerialNumberResponse"/>
        </operation>
        <operation name="generateLicense">
            <input wsam:Action="tns:ILicenseGenerator/generateLicenseRequest" message="tns:generateLicense"/>
            <output wsam:Action="tns:ILicenseGenerator/generateLicenseResponse" message="tns:generateLicenseResponse"/>
        </operation>
    </portType>

    <binding name="LicenseGeneratorImplPortBinding" type="tns:ILicenseGenerator">
        <soap:binding
                style="document"
                transport="http://schemas.xmlsoap.org/soap/http"
        />
        <operation name="getLicenseBySerialNumber">
            <soap:operation soapAction=""/>
            <input>
                <soap:body use="literal"/>
            </input>
            <output>
                <soap:body use="literal"/>
            </output>
        </operation>
        <operation name="generateLicense">
            <soap:operation soapAction=""/>
            <input>
                <soap:body use="literal"/>
            </input>
            <output>
                <soap:body use="literal"/>
            </output>
        </operation>
    </binding>

    <service name="LicenseGeneratorImplService">
        <port name="LicenseGeneratorImplPort" binding="tns:LicenseGeneratorImplPortBinding">
            <soap:address location="http://127.0.0.1:8000/LICENSE"/>
        </port>
    </service>

</definitions>